{
  "extends": "./tsconfig.node.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
    "types": ["vite/client", "./worker-configuration.d.ts"],
    "baseUrl": ".",
    "paths": {
      "@worker/*": ["./src/worker/*"],
      "@db/*": ["./src/db/*"]
    }
  },
  "include": ["src/worker", "src/db"]
}
